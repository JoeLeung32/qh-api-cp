<h1><%= title %></h1>
<div class="table-responsive">
    <table class="table">
        <thead>
        <tr>
            <th>#</th>
            <th>User</th>
            <th>Last Action</th>
            <th>Activity</th>
        </tr>
        </thead>
        <tbody>
        <% for(var i = 0; i < data.length; i++) { %>
            <tr>
                <td><%= data[i].id %></td>
                <td><%= data[i].username %></td>
                <td><%= data[i].updatedAt %></td>
                <td><%= data[i].isValid ? 'true' : 'false' %></td>
            </tr>
        <% } %>

        </tbody>
    </table>
</div>
